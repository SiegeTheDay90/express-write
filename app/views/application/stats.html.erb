<div>
  <canvas id="myChart"></canvas>
</div>

<script src="https://cdn.jsdelivr.net/npm/chart.js"></script>

<script>
  const ctx = document.getElementById('myChart');

  new Chart(ctx, {
    type: 'bar',
    data: {
      labels: [
        <% @labels.each do |label| %>
            "<%= label %>",
        <% end %>
      ]
      ,
      datasets: [{
        label: '# of Page Loads',
        data: <%= @values %>,
        borderWidth: 3
      }]
    },
  });
</script>

